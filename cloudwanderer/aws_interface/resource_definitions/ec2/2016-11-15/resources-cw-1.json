{
  "service": {
    "globalService": false,
    "regionalResources": true
  },
  "resources": {
    "Vpc": {
      "type": "resource",
      "relationships": [
        {
          "basePath": "@",
          "idParts": [
            {
              "path": "DhcpOptionsId"
            }
          ],
          "service": "ec2",
          "resourceType": "dhcp_options",
          "regionSource": "sameAsResource",
          "accountIdSource": "unknown",
          "direction": "outbound"
        }
      ]
    },
    "Subnet": {
      "type": "resource",
      "relationships": [
        {
          "basePath": "@",
          "idParts": [
            {
              "path": "VpcId"
            }
          ],
          "service": "ec2",
          "resourceType": "vpc",
          "regionSource": "sameAsResource",
          "accountIdSource": "unknown",
          "direction": "inbound"
        }
      ]
    },
    "VpcEnableDnsSupport": {
      "type": "secondaryAttribute"
    },
    "Image": {
      "type": "resource",
      "relationships": [
        {
          "basePath": "BlockDeviceMappings[].Ebs",
          "idParts": [
            {
              "path": "SnapshotId"
            }
          ],
          "service": "ec2",
          "resourceType": "snapshot",
          "regionSource": "sameAsResource",
          "accountIdSource": "unknown",
          "direction": "outbound"
        }
      ],
      "defaultFilters": {
        "Owners": ["self"]
      }
    },
    "Snapshot": {
      "type": "resource",
      "defaultFilters": {
        "OwnerIds": ["self"]
      }
    },
    "Instance": {
      "type": "resource",
      "relationships": [
        {
          "basePath": "@",
          "idParts": [
            {
              "path": "ImageId"
            }
          ],
          "service": "ec2",
          "resourceType": "image",
          "regionSource": "sameAsResource",
          "accountIdSource": "unknown",
          "direction": "outbound"
        },
        {
          "basePath": "@",
          "idParts": [
            {
              "path": "KeyName"
            }
          ],
          "service": "ec2",
          "resourceType": "key_pair_info",
          "regionSource": "sameAsResource",
          "accountIdSource": "sameAsResource",
          "direction": "outbound"
        },
        {
          "basePath": "@",
          "idParts": [
            {
              "path": "SubnetId"
            }
          ],
          "service": "ec2",
          "resourceType": "subnet",
          "regionSource": "sameAsResource",
          "accountIdSource": "unknown",
          "direction": "inbound"
        },
        {
          "basePath": "@",
          "idParts": [
            {
              "path": "VpcId"
            }
          ],
          "service": "ec2",
          "resourceType": "vpc",
          "regionSource": "sameAsResource",
          "accountIdSource": "unknown",
          "direction": "inbound"
        },
        {
          "basePath": "@",
          "idParts": [
            {
              "path": "IamInstanceProfile.Arn",
              "regexPattern": "[^:]+:[^:]+:[^:]+:[^:]*:(?P<account_id>[^:]+):[^:]+/(?P<id_part_0>[^:]+)"
            }
          ],
          "service": "iam",
          "resourceType": "instance_profile",
          "regionSource": "unknown",
          "accountIdSource": "sameAsResource",
          "direction": "outbound"
        }
      ]
    }
  }
}
